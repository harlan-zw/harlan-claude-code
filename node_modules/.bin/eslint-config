#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/harlan/pkg/harlan-claude-code/node_modules/.pnpm/@antfu+eslint-config@6.7.3_@vue+compiler-sfc@3.5.26_eslint@9.39.2_typescript@5.9.3/node_modules/@antfu/eslint-config/bin/node_modules:/home/harlan/pkg/harlan-claude-code/node_modules/.pnpm/@antfu+eslint-config@6.7.3_@vue+compiler-sfc@3.5.26_eslint@9.39.2_typescript@5.9.3/node_modules/@antfu/eslint-config/node_modules:/home/harlan/pkg/harlan-claude-code/node_modules/.pnpm/@antfu+eslint-config@6.7.3_@vue+compiler-sfc@3.5.26_eslint@9.39.2_typescript@5.9.3/node_modules/@antfu/node_modules:/home/harlan/pkg/harlan-claude-code/node_modules/.pnpm/@antfu+eslint-config@6.7.3_@vue+compiler-sfc@3.5.26_eslint@9.39.2_typescript@5.9.3/node_modules:/home/harlan/pkg/harlan-claude-code/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/harlan/pkg/harlan-claude-code/node_modules/.pnpm/@antfu+eslint-config@6.7.3_@vue+compiler-sfc@3.5.26_eslint@9.39.2_typescript@5.9.3/node_modules/@antfu/eslint-config/bin/node_modules:/home/harlan/pkg/harlan-claude-code/node_modules/.pnpm/@antfu+eslint-config@6.7.3_@vue+compiler-sfc@3.5.26_eslint@9.39.2_typescript@5.9.3/node_modules/@antfu/eslint-config/node_modules:/home/harlan/pkg/harlan-claude-code/node_modules/.pnpm/@antfu+eslint-config@6.7.3_@vue+compiler-sfc@3.5.26_eslint@9.39.2_typescript@5.9.3/node_modules/@antfu/node_modules:/home/harlan/pkg/harlan-claude-code/node_modules/.pnpm/@antfu+eslint-config@6.7.3_@vue+compiler-sfc@3.5.26_eslint@9.39.2_typescript@5.9.3/node_modules:/home/harlan/pkg/harlan-claude-code/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../.pnpm/@antfu+eslint-config@6.7.3_@vue+compiler-sfc@3.5.26_eslint@9.39.2_typescript@5.9.3/node_modules/@antfu/eslint-config/bin/index.mjs" "$@"
else
  exec node  "$basedir/../.pnpm/@antfu+eslint-config@6.7.3_@vue+compiler-sfc@3.5.26_eslint@9.39.2_typescript@5.9.3/node_modules/@antfu/eslint-config/bin/index.mjs" "$@"
fi
